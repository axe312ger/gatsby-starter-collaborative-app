{
  "name": "gatsby-starter-collaborative-app-server",
  "description": "Clickers server",
  "private": true,
  "version": "1.0.0",
  "author": "Benedikt RÃ¶tsch <info@benedikt-roetsch.de>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "concurrently --kill-others 'npm:dev:db' 'npm:dev:server'",
    "dev:db": "mongod --config /usr/local/etc/mongod.conf",
    "dev:server": "nodemon -x ts-node . -e ts",
    "postinstall": "npm run build",
    "build": "tsc --pretty",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint src/**/*.ts",
    "deploy-production": "cd .. && now --docker --public && now alias"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --config ../.prettierrc",
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "sharedb": "^1.0.0-beta.9",
    "sharedb-access": "^4.2.0",
    "sharedb-mongo": "^1.0.0-beta.4",
    "websocket-json-stream": "0.0.3",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.5",
    "@types/ws": "^5.1.2",
    "concurrently": "^3.6.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.2",
    "prettier": "1.14.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  }
}
