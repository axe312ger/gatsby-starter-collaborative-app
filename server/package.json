{
  "name": "gatsby-starter-collaborative-app-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently --kill-others 'npm:dev:db' 'npm:dev:server'",
    "dev:db": "mongod --config /usr/local/etc/mongod.conf",
    "dev:server": "nodemon -x ts-node .",
    "postinstall": "npm run build",
    "build": "tsc --pretty",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-production": "cd .. && now --docker --public && now alias"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "sharedb": "^1.0.0-beta.9",
    "sharedb-access": "^4.2.0",
    "sharedb-mongo": "^1.0.0-beta.4",
    "websocket-json-stream": "0.0.3",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.5",
    "@types/ws": "^5.1.2",
    "concurrently": "^3.6.0",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.18.2",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1"
  }
}
